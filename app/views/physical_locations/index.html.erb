<h1>Listing physical_locations</h1>

<table>
  <tr>
    <th>Address</th>
    <th>City</th>
    <th>State</th>
    <th>Country</th>
    <th>Zip</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @physical_locations.each do |physical_location| %>
  <tr>
    <td><%= physical_location.address %></td>
    <td><%= physical_location.city %></td>
    <td><%= physical_location.state %></td>
    <td><%= physical_location.country %></td>
    <td><%= physical_location.zip %></td>
    <td><%= physical_location.latitude %></td>
    <td><%= physical_location.longitude %></td>
    <td><%= link_to 'Show', physical_location %></td>
    <td><%= link_to 'Edit', edit_physical_location_path(physical_location) %></td>
    <td><%= link_to 'Destroy', physical_location, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

